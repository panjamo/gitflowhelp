<?xml version="1.0" encoding="utf-8"?>
<ArrayOfCustomAction xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <CustomAction>
    <Caption>git stage diff </Caption>
    <Target>git</Target>
    <Parameters>stagediff</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git flow bugfix start</Caption>
    <Target>gitflowbugfixStart.cmd</Target>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>false</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git flow bugfix finish </Caption>
    <Target>gitflowbugfixFinish.cmd</Target>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>false</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git exclude (for not tracked files)</Caption>
    <Target>git</Target>
    <Parameters>exclude "$FILE"</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git hide</Caption>
    <Target>git</Target>
    <Parameters>hide "$FILE"</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git hidden</Caption>
    <Target>git</Target>
    <Parameters>hidden</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>true</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git hidechanged</Caption>
    <Target>git</Target>
    <Parameters>hidechanged</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git unhideall</Caption>
    <Target>git</Target>
    <Parameters>unhideall</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git flow init</Caption>
    <Target>gitflowinit.cmd</Target>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>false</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>Branches containing this commit</Caption>
    <Target>git</Target>
    <Parameters>branch -r --contains $SHA</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>true</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>Show complete diff for this commit</Caption>
    <Target>git</Target>
    <Parameters>difftool -d $SHA^ $SHA</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git commit --fixup</Caption>
    <Target>git</Target>
    <Parameters>commit --fixup $SHA</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>Show complete diff against working directory</Caption>
    <Target>git</Target>
    <Parameters>difftool -d $SHA^</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>Clean workspace (unknown, modified and ignored) and staging area !!!</Caption>
    <Target>git</Target>
    <Parameters>resetfdx</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git mergetool</Caption>
    <Target>git</Target>
    <Parameters>mergetool $FILE</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>false</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
  <CustomAction>
    <Caption>git reset --hard</Caption>
    <Target>git</Target>
    <Parameters>reset --hard $SHA</Parameters>
    <OpenInSeparateWindow>false</OpenInSeparateWindow>
    <ShowFullOutput>true</ShowFullOutput>
    <IsSilent>true</IsSilent>
  </CustomAction>
</ArrayOfCustomAction>